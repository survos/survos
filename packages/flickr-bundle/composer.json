{
    "name": "survos/flickr-bundle",
    "description": "Symfony bundle that wraps samwilson/phpflickr library",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "twig/twig": "^3.4",
        "samwilson/phpflickr": "^6.0",
        "survos/installer": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Survos\\FlickrBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true
        }
    }
}
