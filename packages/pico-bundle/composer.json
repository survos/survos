{
  "name": "survos/pico-bundle",
  "descripion": "light-weight wrapper around picocss library https://picocss.com/",
  "type": "symfony-bundle",
  "description": "pico-bundle Bundle",
  "keywords": ["symfony-ux", "symfony"],

  "license": "MIT",
  "require": {
    "php": "^8.3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/http-kernel": "^7.3",
    "twig/twig": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Survos\\PicoBundle\\": "src/"
    }
  },
  "require-dev": {
    "symfony/browser-kit": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/phpunit-bridge": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/var-dumper": "^7.3"
  }
}
