{
    "controllers": {
        "@survos/bootstrap-bundle": {
            "search": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            }
        },
        "@survos/simple-datatables-bundle": {
            "table": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@survos/simple-datatables/style.css": true
                }
            }
        },
        "@symfony/ux-chartjs": {
            "chart": {
                "enabled": true,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
