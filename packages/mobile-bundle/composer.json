{
    "name": "survos/mobile-bundle",
    "description": "Bundle with utilities for creating a Symfony-based mobile app",
    "keywords": [
        "symfony-ux"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "twig/twig": "^3.4",
        "survos/core-bundle": "^1.5",
        "knplabs/knp-menu-bundle": "^3.2",
        "symfony/event-dispatcher": "^7.3",
        "symfony/ux-twig-component": "^2.22",
        "symfony/options-resolver": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/var-dumper": "^7.3",
        "symfony/security-bundle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Survos\\MobileBundle\\": "src/"
        }
    }
}
