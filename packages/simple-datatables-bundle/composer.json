{
    "name": "survos/simple-datatables-bundle",
    "description": "incorporate simple datatables, using twig and stimulus",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/config": "^7.0 || ^6.4",
        "symfony/dependency-injection": "^7.0 || ^6.4",
        "symfony/http-kernel": "^7.0 || ^6.4",
        "symfony/options-resolver": "^7.0 || ^6.4",
        "symfony/ux-twig-component": "^2.9",
        "survos/core-bundle": "^1.5",
        "twig/twig": "^3.4",
        "symfony/stimulus-bundle": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Survos\\SimpleDatatables\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.6-dev"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.0 || ^6.4",
        "symfony/framework-bundle": "^7.0 || ^6.4",
        "symfony/phpunit-bridge": "^7.0 || ^6.4",
        "symfony/twig-bundle": "^7.0 || ^6.4",
        "symfony/var-dumper": "^7.0 || ^6.4",
        "rector/rector": "dev-main"
    },
    "prefer-stable": true
}
