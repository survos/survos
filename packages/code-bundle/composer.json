{
  "name": "survos/code-bundle",
  "type": "symfony-bundle",
  "description": "generate code for Symfony applications",
  "keywords": ["dev", "code", "generator", "php"],

  "license": "MIT",
  "require": {
    "php": "^8.4",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/http-kernel": "^7.3",
    "twig/twig": "^3.4",
    "nette/php-generator": "^4.1",
    "roave/better-reflection": "^6.57"
  },
  "autoload": {
    "psr-4": {
      "Survos\\CodeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Survos\\CodeBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "symfony/browser-kit": "^7.2",
    "symfony/framework-bundle": "^7.2",
    "symfony/twig-bundle": "^7.2",
    "symfony/var-dumper": "^7.2",
    "symfony/console": "^7.3",
    "symfony/routing": "^7.2",
    "symfony/security-bundle": "^7.3",
    "doctrine/orm": "^3.5",
    "halaxa/json-machine": "^1.2",
    "league/csv": "^9.23",
    "survos/workflow-helper-bundle": "^1.5",
    "phpunit/phpunit": "^12.2.7",
    "doctrine/doctrine-bundle": "^2.15"
  },
  "version": "1.5.602",
  "config": {
    "allow-plugins": {
      "endroid/installer": false
    }
  }
}
