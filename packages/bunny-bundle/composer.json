{
    "name": "survos/bunny-bundle",
    "description": "Symfony bundle for interacting with BunnyCDN using https://github.com/ToshY/BunnyNet-PHP",
    "keywords": [
        "symfony-ux",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3.1",
        "symfony/http-kernel": "^7.3.1",
        "twig/twig": "^3.21.1",
        "toshy/bunnynet-php": "^7.0.1",
        "zenstruck/bytes": "^1.0",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.19",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3.1",
        "symfony/twig-bundle": "^7.3.1",
        "symfony/var-dumper": "^7.3.1",
        "survos/simple-datatables-bundle": "^1.6",
        "symfony/yaml": "^7.3.1",
        "symfony/http-client": "^7.3.1",
        "phpunit/phpunit": "^12.2.7",
        "knplabs/knp-time-bundle": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\BunnyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\BunnyBundle\\Tests\\": "Tests/"
        }
    },
    "suggest": {
        "survos/simple-datatables-bundle": "searchable/sortable tables when browsing"
    }
}
