{
    "name": "survos/news-api-bundle",
    "description": "Symfony bundle for interacting with NewsApiCDN using https://github.com/ToshY/NewsApiNet-PHP",
    "keywords": [
        "symfony-ux",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "twig/twig": "^3.4",
        "zenstruck/console-extra": "^1.4",
        "zenstruck/bytes": "^1.0",
        "jcobhams/newsapi": "*@dev"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/phpunit-bridge": "^6.4 || ^7.1",
        "symfony/twig-bundle": "^6.4 || ^7.1",
        "symfony/var-dumper": "^6.4 || ^7.1",
        "survos/simple-datatables-bundle": "^1.5",
        "symfony/yaml": "^6.4 || ^7.1",
        "symfony/http-client": "^6.4 || ^7.1",
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\NewsApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\NewsApiBundle\\Tests\\": "Tests/"
        }
    },
    "suggest": {
        "survos/simple-datatables-bundle": "searchable/sortable tables when browsing"
    },
    "repositories": {
        "survos_news_api": {
            "type": "path",
            "url": "/home/tac/g/tacman/newsapi-php"
        }
    }
}
