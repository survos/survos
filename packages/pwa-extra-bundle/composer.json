{
    "name": "survos/pwa-extra-bundle",
    "description": "Extra tools for https://github.com/Spomky-Labs/phpwa",
    "keywords": [
        "symfony-ux",
        "pwa",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "spomky-labs/pwa-bundle": ">=1.2.5",
        "symfony/config": "^6.4 || ^7.3",
        "symfony/dependency-injection": "^6.4 || ^7.3.1",
        "symfony/http-kernel": "^6.4 || ^7.3.1",
        "symfony/asset-mapper": "^6.4 || ^7.3",
        "twig/twig": "^3.21.1",
        "survos/core-bundle": "^1.6",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.19",
        "symfony/browser-kit": "^6.4 || ^7.3",
        "symfony/framework-bundle": "^6.4 || ^7.3.1",
        "symfony/phpunit-bridge": "^6.4 || ^7.3.1",
        "symfony/twig-bundle": "^6.4 || ^7.3.1",
        "symfony/var-dumper": "^6.4 || ^7.3.1",
        "symfony/ux-twig-component": "^2.27"
    },
    "autoload": {
        "psr-4": {
            "Survos\\PwaExtraBundle\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
