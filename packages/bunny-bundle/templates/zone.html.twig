{% extends "base.html.twig" %}

{% block body %}
    <h1>{{ zone.Name }}</h1>
    <h2>{{ path  }}</h2>
    <table class="table">
        <tbody>
        {% for file in files %}
            <tr>
                <td>
                    <a href="{{ path('survos_bunny_zone', {id: zone.Id, zoneName: zone.Name, path: file.ObjectName}) }}">
                        {{ file.ObjectName }}
                    </a>
                </td>
                <td>
                    {{ file.Path }}
                </td>
                <td>
                    {{ file.Length }}
                </td>
                <td>
{#                    {{ dump(file) }}#}
                </td>
            </tr>
        {% endfor %}

        </tbody>

    </table>
{% endblock %}
