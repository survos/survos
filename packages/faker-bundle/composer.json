{
    "name": "survos/faker-bundle",
    "description": "Makes fakerphp/faker methods available via twig",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.20||^2.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "twig/twig": "^3.4",
        "survos/installer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\FakerBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true
        }
    }
}
