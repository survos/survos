{
    "name": "survos/ip2location-bundle",
    "type": "symfony-bundle",
    "description": "Symfony wrapper for ip2location.ip",
    "keywords": [
        "symfony6",
        "ip",
        "geolocation"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "twig/twig": "^3.4",
        "ip2location/ip2location-io-php": "^1.0"
    },
    "suggest": {
        "symfony/cache-contracts": "Cache the lookups"
    },
    "autoload": {
        "psr-4": {
            "Survos\\Ip2LocationBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/phpunit-bridge": "^6.4 || ^7.1",
        "symfony/twig-bundle": "^6.4 || ^7.1",
        "symfony/var-dumper": "^6.4 || ^7.1",
        "symfony/cache-contracts": "^3.4"
    }
}
