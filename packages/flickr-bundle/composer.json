{
    "name": "survos/flickr-bundle",
    "description": "Symfony bundle that wraps samwilson/phpflickr library",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "ext-curl": "*",
        "ext-simplexml": "*",
        "php": "^8.3",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "twig/twig": "^3.4",
        "samwilson/phpflickr": "^6.0",
        "survos/installer": "*",
        "zenstruck/class-metadata": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12 || ^2.0",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/phpunit-bridge": "^6.4 || ^7.1",
        "symfony/security-bundle": "^6.4 || ^7.1",
        "symfony/twig-bundle": "^6.4 || ^7.1",
        "symfony/var-dumper": "^6.4 || ^7.1",
        "symfony/console": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Survos\\FlickrBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true,
            "zenstruck/class-metadata": true
        }
    }
}
