<!DOCTYPE html>
<html>
    <head>
        <title>{{ title|default('Pictures') }}</title>
        <link rel="shortcut icon" href="{{ asset('build/images/favicon.png') }}">
        <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
        <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">

        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/webelopalbum/app.css') }}">
        {% endblock %}

    </head>
    <body data-auto-play-speed="{{ autoPlaySpeed|default(5000) }}">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h1>{{ title|default('Pictures') }}</h1>

                    {% block body %}
                    {% endblock %}

                </div>
            </div>
        </div>

        {{ include('@WebelopAlbum/album/_slideshow.html.twig', {tags: []}) }}

        {% block javascripts %}
            <script src="{{ asset('bundles/webelopalbum/app.js') }}"></script>
        {% endblock %}
    </body>
</html>