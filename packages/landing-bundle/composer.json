{
    "name": "survos/landing-bundle",
    "description": "Add some tools and resources to a Symfony application",
    "type": "symfony-bundle",
    "keywords": [
        "generator",
        "skeleton",
        "symfony5"
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-zip": "*",
        "meyfa/php-svg": "^0.9.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/string": "^6.4 || ^7.1 ",
        "symfony/yaml": "^6.4 || ^7.1 ",
        "twig/string-extra": "^3.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Survos\\LandingBundle\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "id": "landing-bundle.survos.com"
        }
    }
}
