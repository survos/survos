{
    "name": "survos/survos",
    "description": "survos monorepo -- bundles and demos",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        },
        {
            "name": "Nacho Mart\u00edn",
            "email": "nacho@limenius.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "api-platform/core": "^3.2",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "doctrine/persistence": "^2.0 || ^3.0",
        "fakerphp/faker": "^1.20",
        "friendsofsymfony/jsrouting-bundle": "^3.3",
        "gajus/dindent": "^2.0",
        "haydenpierce/class-finder": "^0.4.3",
        "illuminate/collections": "^10.9",
        "jefs42/libretranslate": "^1.1",
        "knplabs/knp-menu-bundle": "^3.2",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "league/csv": "^9.9",
        "picqer/php-barcode-generator": "^2.2",
        "roave/better-reflection": "^6.0",
        "sabre/xml": "^4.0",
        "sebastian/diff": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "survos/bootstrap-bundle": "^1.4",
        "survos/command-bundle": "^1.5",
        "survos/liform": "^1.0",
        "survos/seo-bundle": "^1.5",
        "survos/wikidata": "^4.0",
        "symfony/cache": "^6.1",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/dotenv": "^6.4 || ^7.0",
        "symfony/finder": "^6.4 || ^7.0",
        "symfony/flex": "^2.0",
        "symfony/form": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/http-client": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/messenger": "^6.4 || ^7.0",
        "symfony/options-resolver": "^6.4 || ^7.0",
        "symfony/password-hasher": "^6.4 || ^7.0",
        "symfony/process": "^6.4 || ^7.0",
        "symfony/runtime": "^6.4 || ^7.0",
        "symfony/security-bundle": "^6.4 || ^7.0",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/stimulus-bundle": "^2.21",
        "symfony/stopwatch": "^6.1",
        "symfony/string": "^6.4 || ^7.0",
        "symfony/ux-twig-component": "^2.8",
        "symfony/validator": "^6.4 || ^7.0",
        "symfony/workflow": "^7.0",
        "symfony/yaml": "^6.4 || ^7.0",
        "tacman/twig-tree-tag": "^3.0.1",
        "twig/extra-bundle": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "^3.4",
        "zenstruck/console-extra": "^1.1"
    },
    "require-dev": {
        "ekino\/phpstan-banned-code": "^1.0",
        "php-parallel-lint\/php-parallel-lint": "^1.3",
        "phpdocumentor\/reflection-docblock": "*",
        "phpstan\/extension-installer": "^1.3",
        "phpstan\/phpstan": "^1.8",
        "phpstan\/phpstan-symfony": "^1.3",
        "phpunit\/phpunit": "^9.5||^10.0||^11.0",
        "roave\/security-advisories": "dev-latest",
        "spaze\/phpstan-disallowed-calls": "^2.14",
        "squizlabs\/php_codesniffer": "^3.7",
        "survos\/maker-bundle": "^1.5",
        "symfony\/browser-kit": "^6.4 || ^7.0",
        "symfony\/debug-bundle": "^6.4 || ^7.0",
        "symfony\/maker-bundle": "^1.38",
        "symfony\/phpunit-bridge": "^6.4 || ^7.0",
        "symfony\/polyfill-php80": "^1.16.0",
        "symfony\/security-core": "^6.4 || ^7.0",
        "symfony\/security-csrf": "^6.4 || ^7.0",
        "symfony\/twig-bundle": "^6.4 || ^7.0",
        "symfony\/var-dumper": "^6.4 || ^7.0",
        "symfony\/web-profiler-bundle": "^6.4 || ^7.0",
        "symplify\/monorepo-builder": "*"
    },
    "conflict": {
        "symfony\/symfony": "*"
    },
    "repositories": {
        "velzon": {
            "type": "vcs",
            "url": "git@github.com:survos\/velzon-bundle.git"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src\/",
            "Survos\\KeyValueBundle\\": "packages\/key-value-bundle\/src\/",
            "Survos\\GlobalGivingBundle\\": "packages\/global-giving-bundle\/src\/"
        },
        "exclude-from-classmap": [
            "packages\/location-bundle\/Tests\/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests\/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "endroid/installer": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "survos/installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^7.0"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "coverage": "php -d xpcov.enabled=1 vendor\/bin\/phpunit packages --coverage-html coverage"
    }
}
