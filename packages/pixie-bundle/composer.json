{
    "name": "survos/pixie-bundle",
    "description": "Symfony Bundle to easily allow a SQLite Key Value lookup.  Optionally uses zip and bunny utilities for sync",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "prefer-stable": true,
    "require": {
        "ext-pdo": "*",
        "php": "^8.3",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "twig/twig": "^3.4",
        "survos/installer": "^1.5",
        "symfony/yaml": "^6.4 || ^7.1",
        "doctrine/dbal": "^3.0|^4.0",
        "symfony/property-access": "^6.4 || ^7.1",
        "halaxa/json-machine": "^1.1",
        "league/csv": "^9.9",
        "symfony/twig-bundle": "^6.4 || ^7.1",
        "symfony/routing": "^6.4 || ^7.1",
        "symfony/serializer": "^6.4 || ^7.1",
        "symfony/serializer-pack": "^1.3",
        "zenstruck/console-extra": "^1.4",
        "symfony/ux-icons": "^2.21",
        "symfony/http-client-contracts": "^3.5",
        "symfony/http-client": "^6.4 || ^7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/var-dumper": "^6.4 || ^7.1",
        "phpunit/phpunit": "^11.4",
        "symfony/phpunit-bridge": "^6.4 || ^7.1",
        "symfony/stopwatch": "^6.4 || ^7.1",
        "survos/workflow-helper-bundle": "^1.5",
        "symfony/ux-chartjs": "^2.21",
        "survos/bootstrap-bundle": "^1.5",
        "survos/api-grid-bundle": "^1.5",
        "symfony/event-dispatcher": "^6.4 || ^7.1",
        "symfony/messenger": "^6.4 || ^7.1",
        "survos/bunny-bundle": "^1.5",
        "zenstruck/filesystem": "1.x-dev",
        "league/flysystem-ziparchive": "3.x-dev",
        "psr/log": "^3.0",
        "survos/scraper-bundle": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Survos\\PixieBundle\\": "src/",
            "Survos\\PixieBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true,
            "endroid/installer": true
        }
    },
    "minimum-stability": "dev"
}
