{
    "name": "survos/tree-bundle",
    "description": "incorporate jstree, using twig and stimulus and api-platform",
    "keywords": [
        "symfony-ux",
        "hierarchical"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^3.3",
        "doctrine/persistence": "^2.0 || ^3.0 || ^4.0",
        "stof/doctrine-extensions-bundle": "^1.8",
        "survos/core-bundle": "^1.5",
        "survos/inspection-bundle": "^1.5",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/options-resolver": "^7.3",
        "symfony/ux-twig-component": "^2.8",
        "tacman/twig-tree-tag": "^3.0.1",
        "twig/twig": "^3.4",
        "symfony/stimulus-bundle": "^2.21"
    },
    "require-dev": {
        "api-platform/core": "^4.0",
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/var-dumper": "^7.3",
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\Tree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\Tree\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        },
        "symfony": {
            "require": "^7.0 || ^6.3"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
