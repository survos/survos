{
    "name": "survos/pixie-bundle",
    "description": "Symfony Bundle allow allow managing museum data in an independent SQLite database using a doctrine ORM",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "prefer-stable": true,
    "require": {
        "ext-pdo": "*",
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "twig/twig": "^3.4",
        "survos/installer": "^1.5 || ^2.0",
        "symfony/yaml": "^7.3",
        "doctrine/dbal": "^3.0|^4.0",
        "symfony/property-access": "^7.3",
        "halaxa/json-machine": "^1.1",
        "league/csv": "^9.9",
        "symfony/twig-bundle": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/serializer": "^7.3",
        "symfony/serializer-pack": "^1.3",
        "symfony/ux-icons": "^2.21",
        "symfony/http-client-contracts": "^3.5",
        "symfony/http-client": "^7.3",
        "symfony/asset": "^7.1",
        "illuminate/collections": "^11.33",
        "alexandre-fernandez/json-translation-bundle": "^1.0",
        "symfony/object-mapper": "^7.3",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/var-dumper": "^7.3",
        "phpunit/phpunit": "^11.4",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/stopwatch": "^7.3",
        "survos/workflow-helper-bundle": "^1.5",
        "symfony/ux-chartjs": "^2.21",
        "survos/bootstrap-bundle": "^1.5",
        "survos/api-grid-bundle": "^1.5",
        "symfony/event-dispatcher": "^7.3",
        "symfony/messenger": "^7.3",
        "survos/bunny-bundle": "^1.5",
        "zenstruck/filesystem": "1.x-dev",
        "league/flysystem-ziparchive": "3.x-dev",
        "psr/log": "^3.0",
        "survos/scraper-bundle": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.64",
        "meilisearch/meilisearch-php": "^1.11",
        "symfony/security-bundle": "^7.3",
        "rector/rector": "dev-main",
        "survos/libre-translate-bundle": "^1.5",
        "doctrine/orm": "^3.3",
        "survos/sais-bundle": "^1.5",
        "stof/doctrine-extensions-bundle": "^1.13",
        "api-platform/core": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Survos\\PixieBundle\\": "src/",
            "Survos\\PixieBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true,
            "endroid/installer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev"
}
