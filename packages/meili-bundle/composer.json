{
    "name": "survos/meili-bundle",
    "description": "Utilities for managing a meilisearch indexes",
    "keywords": [
        "symfony-ux"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "symfony/console": "^7.3",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "twig/twig": "^3.4",
        "survos/installer": "^2.1.2",
        "symfony/property-access": "^7.2",
        "meilisearch/meilisearch-php": "^1.14",
        "adbario/php-dot-notation": "^3.3",
        "cebe/php-openapi": "^1.8",
        "survos/core-bundle": "^1.5",
        "survos/inspection-bundle": "^1.5",
        "nyholm/psr7": "^1.8",
        "zenstruck/bytes": "^1.0",
        "symfony/stopwatch": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/var-dumper": "^7.3",
        "rector/rector": "^2.0",
        "symfony/ux-chartjs": "^2.23",
        "symfony/http-client": "^7.2",
        "api-platform/doctrine-orm": "^4.1",
        "symfony/intl": "^7.3",
        "symfony/ux-twig-component": "^2.27",
        "doctrine/doctrine-bundle": "^2.15",
        "symfony/messenger": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Survos\\MeiliBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true,
            "php-http/discovery": true
        }
    }
}
