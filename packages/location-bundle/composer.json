{
    "name": "survos/location-bundle",
    "type": "symfony-bundle",
    "description": "Use Geonames to in an application",
    "keywords": [
        "symfony",
        "city",
        "state"
    ],
    "homepage": "https://github.com/survos/SurvosLocationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/orm": "^2.12 || ^3.0",
        "doctrine/persistence": "^2.0 || ^3.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/console": "^6.4 || ^7.1",
        "symfony/doctrine-bridge": "^6.4 || ^7.1",
        "symfony/flex": "^2.0",
        "symfony/form": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/validator": "^6.4 || ^7.1",
        "symfony/yaml": "^6.4 || ^7.1"
    },
    "require-dev": {
        "api-platform/core": "^3.1.11",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.2",
        "survos/api-grid-bundle": "^1.5.392",
        "symfony/phpunit-bridge": "^6.4 || ^7.1",
        "symplify/config-transformer": "^11.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Survos\\LocationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\LocationBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "prefer-stable": true
}
