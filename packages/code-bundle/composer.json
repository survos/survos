{
  "name": "survos/code-bundle",
  "type": "symfony-bundle",
  "description": "generate code for Symfony applications",
  "keywords": ["dev", "code", "generator", "php"],

  "license": "MIT",
  "require": {
    "php": "^8.3",
    "symfony/config": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "symfony/http-kernel": "^7.2",
    "twig/twig": "^3.4",
    "nette/php-generator": "^4.1",
    "roave/better-reflection": "^6.57"
  },
  "autoload": {
    "psr-4": {
      "Survos\\CodeBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "symfony/browser-kit": "^7.2",
    "symfony/framework-bundle": "^7.2",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/twig-bundle": "^7.2",
    "symfony/var-dumper": "^7.2",
    "symfony/console": "^7.3",
    "symfony/routing": "^7.2",
    "symfony/security-bundle": "^7.3@beta"
  },
  "minimum-stability": "beta",
  "version": "2.0.0"
}
